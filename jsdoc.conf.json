{
  "tags": {
    "sourceType": "module",
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "package.json",
      "lib/",
      "README.md"
    ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  },
  "recurse": true
}